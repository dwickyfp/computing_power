# Worker Service Environment Variables
# Copy this file to .env and adjust values

# Database (Config DB - same as backend)
DATABASE_URL=postgresql://postgres:postgres@localhost:5433/postgres

# Connection Pool - HIGH PERFORMANCE
DB_POOL_SIZE=10
DB_MAX_OVERFLOW=10

# Redis
REDIS_URL=redis://localhost:6379/0
CELERY_BROKER_URL=redis://localhost:6379/1
CELERY_RESULT_BACKEND=redis://localhost:6379/2

# Security (must match backend's key)
CREDENTIAL_ENCRYPTION_KEY=your-32-byte-encryption-key-here

# Worker Settings - HIGH PERFORMANCE
WORKER_CONCURRENCY=8
TASK_SOFT_TIME_LIMIT=120
TASK_HARD_TIME_LIMIT=180
PREVIEW_ROW_LIMIT=500

# DuckDB Settings - HIGH PERFORMANCE
DUCKDB_MEMORY_LIMIT=2GB
DUCKDB_THREADS=4

# Health API Server
SERVER_HOST=0.0.0.0
SERVER_PORT=8002

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json
